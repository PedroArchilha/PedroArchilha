var senha, repete, cont = 1, senha_login, acessar;

do {
  senha = prompt("Digite uma senha com 6 digitos: ");
} while(senha.length != 6);

repete = prompt("Repita a senha de 6 digitos: ");

while(repete != senha){
  repete = prompt("A senha nao e a mesma. Digite novamente: ");
}

alert("Senha criada. Agora voce pode acessar sua conta.");

acessar = prompt("Deseja acessar sua conta? Digite S para SIM e N para NAO: ")

if(acessar == "S"){
  senha_login = prompt("Entre com sua senha para acessar a sua conta: ");

for(;senha != senha_login && cont < 3; cont++){ //
  alert("A senha informada nao confere. Insira novamente. Voce tem mais " + (3-cont) + " tentativa(s).")
  senha_login = prompt("Para acessar sua conta, entre com sua senha: ");
}
if(cont < 3){
  alert("Acesso permitido");
} else{
  senha = 99;
  alert("Senha bloqueada. Entre em contato com seu gerente.");
}
} else{
  alert("Tenha um bom dia de trabalho.")
}
